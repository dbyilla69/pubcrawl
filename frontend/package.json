{
	"name": "frontend",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"dev": "next -p 7777",
		"build": "next build",
		"start": "next start",
		"test": "NODE_ENV=test jest --watch",
		"test-win": "SET NODE_ENV=test && jest --watch"
	},
	"dependencies": {
		"apollo-client": "^2.6.4",
		"graphql-tag": "^2.10.1",
		"next": "^9.0.6",
		"react": "^16.9.0",
		"react-apollo": "^3.1.1",
		"react-dom": "^16.9.0",
		"styled-components": "^4.4.0"
	},
	"devDependencies": {
		"eslint": "^6.4.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.14.3",
		"graphql-tools": "^4.0.5"
	},
	"babel": {
		"env": {
			"development": {
				"presets": [
					"next/babel"
				],
				"plugins": [
					[
						"styled-components",
						{
							"ssr": true,
							"displayName": true
						}
					]
				]
			},
			"production": {
				"presets": [
					"next/babel"
				],
				"plugins": [
					[
						"styled-components",
						{
							"ssr": true,
							"displayName": true
						}
					]
				]
			},
			"test": {
				"presets": [
					[
						"next/babel",
						{
							"preset-env": {
								"modules": "commonjs"
							}
						}
					]
				],
				"plugins": [
					[
						"styled-components",
						{
							"ssr": true,
							"displayName": true
						}
					]
				]
			}
		}
	}
}
